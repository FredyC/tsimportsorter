{
  "groupRules": [
    { "flag": "all", "regex": "^[tuv]", "subGroups": [{ "flag": null, "regex": "^v" }, "^u"] },
    { "flag": "all", "regex": "^[rs]", "subGroups": [{ "flag": "all", "regex": "^r" }] },
    { "flag": "all", "regex": "^[pq]", "subGroups": [{ "flag": "all", "regex": "^q" }] },
    { "flag": "all", "regex": "^[no]", "subGroups": ["^o", { "flag": "scripts", "regex": "^n" }] },
    { "flag": "scripts", "regex": "^[klm]", "subGroups": [{ "flag": null, "regex": "^l" }, "^k"] },
    { "flag": "scripts", "regex": "^[ij]", "subGroups": [{ "flag": "all", "regex": "^i" }] },
    { "flag": "scripts", "regex": "^[gh]", "subGroups": [{ "flag": "scripts", "regex": "^g" }] },
    { "regex": "^[ef]", "subGroups": [{ "flag": null, "regex": "^f" }] },
    { "regex": "^[cd]", "subGroups": [{ "flag": "all", "regex": "^d" }] },
    { "regex": "^[ab]", "subGroups": [{ "flag": "scripts", "regex": "^a" }, "^b"] }
  ]
}
